{
  "name": "amylian/yii2-amylian-doctrine-cache",
  "type": "yii2-extension",
  "description": "ATTENTION EXPERIMENTAL! Cache implementations for Amylian/Doctrine",
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require": {
    "amylian/yii2-amylian-doctrine-base": "*",
    "amylian/yii2-amylian-cache": "*",
    "doctrine/cache": "*"
  },
  "require-dev": {
    "amylian/yii2-amylian-phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "abexto\\amylian\\yii\\doctrine\\cache\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "abexto\\amylian\\yii\\doctrine\\cache\\tests\\": "tests/",
      "Doctrine\\Tests\\": "vendor/doctrine/cache/tests/Doctrine/Tests/",
    }
  }
}
